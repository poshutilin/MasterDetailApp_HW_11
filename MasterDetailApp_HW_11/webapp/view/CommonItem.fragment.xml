<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core"
	xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" controllerName="jetcourses.MasterDetailApp.controller.Detail">
	<Dialog id="generateAnnotations" title="{commonItem>/title}" contentWidth="400px" contentHeight="135px">
		<beginButton>
			<Button text="{i18n>btnOk}" press="onPressOKCreate"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>btnCancel}" press="onPressCancel"/>
		</endButton>
		<content>
			<VBox class="sapUiSmallMargin">
				<smartForm:SmartForm editable="true">
					<smartForm:Group>
						<smartForm:GroupElement>
							<smartField:SmartField id="commonItemSmartField" />
						</smartForm:GroupElement>
					</smartForm:Group>
				</smartForm:SmartForm>
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>